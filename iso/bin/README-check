#! /bin/bash

# Display files that aren't documented in README

for X in `
    ls -1 | 
        egrep -v "(CVS|_Inline|.cvsignore|.swp|README-check|README.txt|.pyc|Makefile|^fastplan$|^fastplan-coopt$|^fastplan-coopt-debug$|^fastplan-debug$|^fastplan-profile$)" | 
        sed "s#./##"
    `
do 
    egrep "^$X/? " README.txt >/dev/null || echo $X
done

