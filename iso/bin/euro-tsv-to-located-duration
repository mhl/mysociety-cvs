#!/usr/bin/perl -w

use strict;

# Skip heading
<>;

while (<>) {
    chomp;
    my ($geonameid, $code, $name, $population, $wikipedia, $lat, $lon, $seconds, $rest_of_line) = split /\t/;

    if (defined($lat) && defined($lon) && defined($seconds)) {
        my $mins = $seconds / 60;
        print "$lat $lon $seconds $name, $mins mins, population $population\n";
    }
}

